{
  "name": "WeWereHere",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "openXcode": "cd ios; open Hippopulse.xcworkspace",
    "pod_installer": "npx pod-install ios",
    "iPhone6s": "react-native run-ios --simulator \"iPhone 6s\"",
    "ios_staging_iphone_6s": "yarn iPhone6s --scheme StagingBookKaru --configuration StagingDebug",
    "iPhone11ProMax": "npx react-native run-ios --simulator \"iPhone 11 Pro Max\"",
    "podCleaner": "cd ios; pod deintegrate; rm -rf Podfile.lock; cd .. ; yarn pod_installer",
    "android_run_staging": "yarn addFlipperReduxIn; npx react-native run-android --variant=stagingDebug",
    "android_run_production": "npx react-native run-android --variant=productionDebug",
    "android_bundle_production": "cd android; ./gradlew bundleProductionRelease && cd app/build/outputs/bundle/productionRelease && cp -R ./ ~/Desktop && cd ~/Desktop && open .;",
    "android_bundle_staging": "yarn shotFlipperAway; cd android; ./gradlew bundleStagingRelease && cd app/build/outputs/bundle/stagingRelease && cp -R ./ ~/Desktop && cd ~/Desktop && open .;",
    "release_build_android_staging": "yarn shotFlipperAway; cd android && ./gradlew assembleStagingRelease && cd app/build/outputs/apk/staging/release && cp -R ./ ~/Desktop && cd ~/Desktop && open .;",
    "release_build_android_production": "cd android && ./gradlew assembleProductionRelease && cd app/build/outputs/apk/production/release && cp -R ./ ~/Desktop && cd ~/Desktop && open .;",
    "clean_android_cache": "cd android; ./gradlew clean;",
    "shotFlipperAway": "yarn remove redux-flipper react-native-flipper",
    "addFlipperReduxIn": "yarn add redux-flipper react-native-flipper",
    "rnuc_staging": "$(npm bin)/rnuc .env.staging.yaml",
    "rnuc_production": "$(npm bin)/rnuc .env.yaml",
    "gradlewClean": "cd android && ./gradlew clean && cd ..",
    "assembleRelease": "cd android && ./gradlew assembleRelease && open app/build/outputs/apk/release && cd ..",
    "bundleRelease": "cd android && ./gradlew bundleRelease && open app/build/",
    "installLibs": "yarn add @react-native-async-storage/async-storage @react-native-community/masked-view @react-navigation/bottom-tabs @react-navigation/native @react-navigation/stack react-native-gesture-handler react-native-reanimated react-native-safe-area-context react-native-screens react-native-elements react-native-vector-icons react-redux redux redux-devtools-extension redux-logger redux-persist redux-saga"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.8",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-google-signin/google-signin": "^7.0.1",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@react-navigation/stack": "^6.0.11",
    "@twotalltotems/react-native-otp-input": "^1.3.11",
    "axios": "^0.21.4",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-native": "0.65.1",
    "react-native-animatable": "^1.3.3",
    "react-native-calendars": "^1.1268.0",
    "react-native-camera": "^4.2.1",
    "react-native-device-info": "^8.4.0",
    "react-native-document-picker": "^7.1.1",
    "react-native-elements": "^3.4.2",
    "react-native-fast-image": "^8.5.11",
    "react-native-fbsdk-next": "^4.6.0",
    "react-native-geolocation-service": "^5.3.0-beta.3",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-crop-picker": "^0.37.2",
    "react-native-image-picker": "^4.1.1",
    "react-native-keyboard-aware-scrollview": "^2.1.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-maps": "^0.29.3",
    "react-native-modal-datetime-picker": "^11.0.0",
    "react-native-modalize": "^2.0.8",
    "react-native-rate": "^1.2.9",
    "react-native-render-html": "^6.3.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.7.2",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-vector-icons": "^8.1.0",
    "react-native-video": "^5.2.0",
    "react-native-video-editing": "^1.0.1",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/runtime": "^7.15.4",
    "@react-native-community/eslint-config": "^3.0.1",
    "babel-jest": "^27.2.0",
    "eslint": "^7.32.0",
    "jest": "^27.2.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
